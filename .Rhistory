lcms_dataset <- lcms_dataset_load(file_name)
path <- tempdir()
lcms_rearrange_datafiles_by_class(lcms_dataset = lcms_dataset,
dataDir = path)
x = list.files(path = paste(path, list.files(path = path),
sep = "/"), full.names = TRUE)
fileList <- x[grep(pattern =".mzXML$", x)]
fileList
